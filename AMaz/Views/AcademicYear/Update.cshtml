@using AMaz.Common
@model UpdateAcademicYearViewModel

@{
    ViewData["Title"] = "Update Academic Year";
    Layout = "_AdminLayout";
}

<h1>Update Academic Year</h1>
<div class="main-content">
    <section class="section">
        <div class="section-header">
            <h1>Create Account</h1>
            <div class="section-header-breadcrumb">
                <div class="breadcrumb-item active"><a href="#">Functions</a></div>
                <div class="breadcrumb-item"><a href="#">Auth</a></div>
                <div class="breadcrumb-item">Create Account</div>
            </div>
        </div>

        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-header">
                        <h4>Create New User</h4>
                    </div>
                    <div class="card-body">
                        <form asp-controller="AcademicYear" asp-action="Update" method="post" asp-route-id ="@(ViewBag.Id)">
                            <div class="form-group">
                                <label asp-for="DateTimeFrom"></label>
                                <input asp-for="DateTimeFrom" class="form-control" />
                            </div>
                            <div class="form-group">
                                <label asp-for="DateTimeTo"></label>
                                <input asp-for="DateTimeTo" class="form-control" />
                            </div>
                            <button type="submit" class="btn btn-primary">Update</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div

